@prefix codemeta: <https://w3id.org/codemeta/3.0/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix semapv: <https://w3id.org/semapv/vocab/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sssom: <https://w3id.org/sssom/> .
@prefix subject: <https://pypi.org/ns/core-metadata#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

rdfs:comment a owl:AnnotationProperty .

sssom:confidence a owl:AnnotationProperty .

sssom:mapping_justification a owl:AnnotationProperty .

sssom:object_label a owl:AnnotationProperty .

sssom:subject_label a owl:AnnotationProperty .

subject:author skos:exactMatch schema1:name .

subject:author_email skos:exactMatch schema1:email .

subject:bugtruck_url skos:exactMatch codemeta:issueTracker .

subject:description skos:exactMatch schema1:description .

subject:download_url skos:exactMatch codemeta:downloadURL .

subject:funding skos:closeMatch codemeta:project_urls .

subject:keywords skos:exactMatch schema1:keywords .

subject:license_expression skos:closeMatch schema1:name .

subject:maintainer skos:exactMatch schema1:name .

subject:maintainer_email skos:closeMatch codemeta:exact .

subject:name skos:exactMatch schema1:name .

subject:platform skos:exactMatch schema1:operatingSystem .

subject:project_urls skos:closeMatch schema1:codeRepository .

subject:requires_dist skos:closeMatch schema1:softwareRequirements .

subject:requires_python skos:narrower schema1:softwareRequirements .

subject:summary skos:narrower codemeta:headline .

subject:description_content_type skos:exactMatch schema1:fileFormat,
        codemeta:encodingFormat .

subject:homepage skos:closeMatch schema1:codeRepository,
        schema1:identifier .

subject:license skos:closeMatch schema1:name ;
    skos:exactMatch schema1:license .

subject:version skos:exactMatch schema1:softwareVersion,
        schema1:version .

subject:docs_url skos:closeMatch schema1:releaseNotes,
        codemeta:buildInstructions,
        codemeta:readme .

subject:classifiers skos:broader codemeta:licence ;
    skos:closeMatch schema1:operatingSystem,
        schema1:programmingLanguage ;
    skos:narrower schema1:softwareRequirements .

[] a owl:Axiom ;
    rdfs:comment "It may include the name in the string.The codemeta term has a path maintainer/exact." ;
    owl:annotatedProperty skos:closeMatch ;
    owl:annotatedSource subject:maintainer_email ;
    owl:annotatedTarget codemeta:exact ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "exact" ;
    sssom:subject_label "maintainer_email" .

[] a owl:Axiom ;
