    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "name" ;
    sssom:subject_label "license" .

[] a owl:Ontology ;
    dcterms:description "Mapping between codemeta and core metadata specification of the PyPa Package Distribution Metadata (https://packaging.python.org/en/latest/specifications/core-metadata/). We have followed the structure of the JSON provided by the API(e.g. https://pypi.org/pypi/somef/json)" ;
    dcterms:license "https://creativecommons.org/publicdomain/zero/1.0/"^^xsd:anyURI ;
    pav:authoredOn "2025-09-10"^^xsd:date ;
    sssom:mapping_provider "https://codemeta.github.io/"^^xsd:anyURI ;
    sssom:mapping_set_id "https://w3id.org/codemeta/crosswalks/PyPa"^^xsd:anyURI .

